{
  "name": "p5-project",
  "version": "0.0.0",
  "scripts": {
    "build": "rimraf ./build && eslint *.js && webpack --mode production --config ./configuration/webpack.config.prod.js",
    "dev": "TASK=develop webpack-dev-server --mode development",
    "dev:mobile": "TASK=develop webpack-dev-server --host 192.168.1.77 --mode development",
    "develop": "configuration/webpack.config.common.js webpack-dev-server --config ./configuration/webpack.config.develop.js --open",
    "serve": "webpack --config ./webpack.config.prod.js -p --progress --profile --bail && http-server ./build -o",
    "test:unit": "rimraf ./reports && karma start",
    "analyze": "webpack-bundle-analyzer ./build/stats.json"
  },
